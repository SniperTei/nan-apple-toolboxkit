target 'NanToolboxKit' do
  # Comment the next line if you don't want to use dynamic frameworks
  project './NanToolboxKit/NanToolboxKit.xcodeproj'
  use_frameworks!

  # Pods for NanToolboxKit
  # pod 'FMDB'

end

target 'NanToolboxKitDemo' do
  project './NanToolboxKitDemo/NanToolboxKitDemo.xcodeproj'
  use_frameworks!
   
  # Pods for NanToolboxKitDemo
  pod 'Alamofire'
end


